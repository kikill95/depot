<% unless current_user %>
    <%= form_for @session, url: session_path, remote: true do |f| %>
      <%= f.text_field :email %>
      <%= @session.errors[:email].first %>
      <%= f.password_field :password %>
      <%= @session.errors[:password].first %>
      <%= f.button 'OK', data: { disable_with: 'Loading...' } %>
    <% end %>
<% end %>
